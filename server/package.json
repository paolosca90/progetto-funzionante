{
  "name": "aicash-revolution-server",
  "version": "1.0.0",
  "description": "AI-powered trading signal generation and execution system",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma migrate reset --force",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write 'src/**/*.js'",
    "build": "echo 'No build step required for Node.js'",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "trading",
    "ai",
    "signals",
    "forex",
    "mt5",
    "fintech"
  ],
  "author": "AI Cash Revolution Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "axios": "^1.6.2",
    "redis": "^4.6.10",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "socket.io": "^4.7.4",
    "nodemailer": "^6.9.7",
    "crypto": "^1.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "prisma": "^5.7.1",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aicash-revolution/server.git"
  },
  "bugs": {
    "url": "https://github.com/aicash-revolution/server/issues"
  },
  "homepage": "https://github.com/aicash-revolution/server#readme"
}